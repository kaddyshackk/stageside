{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["^build", "^test"]
    },
    "docker:build": {
      "dependsOn": ["^test", "^docker:build"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}