{
  "Pipeline": {
    "Dispatching": {
      "DelayIntervalSeconds": 15
    },
    "DynamicCollection": {
      "DelayIntervalSeconds": 2
    },
    "Transformation": {
      "DelayIntervalSeconds": 1,
      "MinBatchSize": 25,
      "MaxBatchSize": 200
    },
    "Processing": {
      "DelayIntervalSeconds": 1,
      "MinBatchSize": 25,
      "MaxBatchSize": 200
    }
  },
  "Queue": {
    "BatchDequeueMaxWaitSeconds": 1,
    "BatchDequeueDelayIntervalMilliseconds": 10,
    "Thresholds": {
      "Collection": { "Normal": 2000, "Warning": 3000, "Critical": 4000 },
      "DynamicCollection": { "Normal": 2000, "Warning": 3000, "Critical": 4000 },
      "Transformation": { "Normal": 2000, "Warning": 3000, "Critical": 4000 },
      "Processing": { "Normal": 2000, "Warning": 3000, "Critical": 4000 }
    }
  },
  "WebBrowser": {
    "Context": {
      "UserAgent": "",
      "ViewportSize": {
        "Width": 1920,
        "Height": 1080
      }
    },
    "Launch": {
      "Headless": true,
      "Args": []
    },
    "Resource": {
      "BrowserConcurrency": 5,
      "ContextConcurrency": 5,
      "ContextStrategy": "Reuse"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning"
    }
  }
