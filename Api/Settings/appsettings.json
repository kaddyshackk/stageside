{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApiOptions": {
    "TimeoutSeconds": 30,
    "RetryAttempts": 3
  },
  "DataSyncOptions": {
    "BronzeRecordIngestion": {
      "BatchSize": 500,
      "FlushIntervalSeconds": 10,
      "QueueTimeoutSeconds": 10
    },
    "PunchupCollection": {
      "Concurrency": 5
    }
  }
}
